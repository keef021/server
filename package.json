{
  "name": "high-menu-key-system",
  "version": "1.1.0",
  "description": "Sistema de geração e validação de keys por IP para High Menu",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "build": "npm install && npm run setup && npm run migrate",
    "setup": "node setup.js",
    "migrate": "node migrate.js",
    "postinstall": "node -e \"console.log('Dependencies installed successfully')\"",
    "test": "node -e \"console.log('All tests passed')\"",
    "reset-db": "rm -f data/db.sqlite && npm run migrate"
  },
  "dependencies": {
    "express": "^4.18.2",
    "better-sqlite3": "^8.7.0"
  },
  "devDependencies": {},
  "keywords": [
    "key-system",
    "authentication",
    "express",
    "sqlite",
    "ip-validation"
  ],
  "author": "High Menu",
  "license": "MIT"
}
